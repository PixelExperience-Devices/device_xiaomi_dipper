<!-- These resources are around just to allow their values to be customized
     for different hardware and product builds. -->
<resources>
    <!-- Should "4G" be shown instead of "LTE" when the network is NETWORK_TYPE_LTE? -->
    <bool name="config_show4GForLTE">true</bool>

    <!-- Location on the screen of the center of the physical power button. -->
    <dimen name="physical_power_button_center_screen_location_y">935px</dimen>

    <!-- Location on the screen of the center of the physical volume up/down buttons. -->
    <dimen name="physical_volume_up_button_center_screen_location_y">485px</dimen>
    <dimen name="physical_volume_down_button_center_screen_location_y">625px</dimen>

    <!-- StatusBar burn-in protection -->
    <bool name="config_statusBarBurnInProtection">true</bool>
    <integer name="config_shift_interval">15</integer>
</resources>
